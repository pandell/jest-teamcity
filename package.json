{
  "name": "jest-teamcity",
  "version": "1.0.0",
  "description": "Reporter for Jest that outputs TeamCity annotations",
  "author": "amaechler <amaechler@gmail.com>",
  "license": "MIT",
  "main": "dist/bundle.js",
  "sideEffects": false,
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.0",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "jest": "^26.0.0",
    "path-browserify": "^1.0.0",
    "prettier": "^2.0.0",
    "ts-jest": "^26.0.0",
    "ts-loader": "^8.0.0",
    "typescript": "^4.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.0.0"
  },
  "scripts": {
    "build": "tsc",
    "bundle": "webpack --mode=production",
    "fix": "prettier \"**/*.{js,json,ts}\" --write",
    "format": "prettier \"**/*.{js,json,ts}\" --check",
    "lint": "eslint --max-warnings 0 --ext \".js,.ts\" --cache .",
    "test": "yarn build && jest",
    "verify": "yarn build && yarn test && yarn lint && yarn format",
    "watch": "tsc --watch"
  }
}
