{
  "name": "@pandell/jest-teamcity",
  "version": "0.0.0",
  "description": "Modern Jest Test Result Processor for TeamCity",
  "keywords": [
    "jest",
    "reporter",
    "teamcity",
    "typescript"
  ],
  "homepage": "https://github.com/pandell/jest-teamcity",
  "repository": {
    "type": "git",
    "url": "https://github.com/pandell/jest-teamcity"
  },
  "license": "MIT",
  "author": {
    "name": "Pandell Technology",
    "url": "http://pandell.com/"
  },
  "contributors": [
    {
      "name": "Andreas Maechler",
      "url": "https://github.com/amaechler"
    }
  ],
  "sideEffects": false,
  "main": "dist/bundle.js",
  "scripts": {
    "build": "tsc",
    "bundle": "yarn clobber && yarn && webpack --mode=production",
    "clobber": "git -c core.longpaths=true clean -dfx",
    "format-fix": "prettier \"**/*.{js,json,ts}\" --write",
    "format": "prettier \"**/*.{js,json,ts}\" --check",
    "lint": "eslint --max-warnings 0 --ext \".js,.ts\" --cache .",
    "postversion": "git push --tags && yarn publish . --tag $npm_package_version && git push && echo \"Successfully released version $npm_package_version!\"",
    "test": "yarn build && jest",
    "verify": "yarn build && yarn test && yarn lint && yarn format",
    "watch": "tsc --watch"
  },
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.0",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "jest": "^26.0.0",
    "path-browserify": "^1.0.0",
    "prettier": "^2.0.0",
    "ts-jest": "^26.0.0",
    "ts-loader": "^8.0.0",
    "typescript": "^4.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
