{
  "name": "jest-teamcity",
  "version": "1.0.0",
  "description": "Reporter for Jest that outputs TeamCity annotations",
  "author": "amaechler <amaechler@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.0",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "jest": "^26.0.0",
    "prettier": "^2.0.0",
    "ts-jest": "^26.0.0",
    "typescript": "^4.0.0"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier \"**/*.{js,json,ts}\" --check",
    "fix": "prettier \"**/*.{js,json,ts}\" --write",
    "test": "jest",
    "lint": "eslint --max-warnings 0 --ext \".js,.ts\" --cache .",
    "verify": "yarn build && yarn test && yarn lint && yarn format",
    "watch": "tsc --watch"
  }
}
